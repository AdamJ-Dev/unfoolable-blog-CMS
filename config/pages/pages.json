{
  "home": {
    "path":  "/"
  },
  "blogs": {
    "path": "/blogs"
  },
  "login": {
    "path": "/login"
  },
  "signup": {
    "path": "/signup"
  },
  "drafts": {
    "path": "/blogs/drafts"
  },
  "workspace": {
    "path": "/blog/workspace/:id",
